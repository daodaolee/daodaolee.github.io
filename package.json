{
  "name": "vitepress-theme-minimalist",
  "version": "1.0.0",
  "packageManager": "pnpm@7.16.0",
  "description": "a minimalist vitepress theme",
  "author": {
    "name": "DaoDaoLee",
    "email": "im@daodaolee.cn",
    "url": "https://daodaolee.cn"
  },
  "license": "MIT",
  "keywords": [
    "vitepress",
    "theme"
  ],
 
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/vitepress/config/baseConfig.js",
    "./types": "./src/types/index.ts",
    "./src/*": "./src/*"
  },
  "main": "./src/index.ts",
  "files": [
    "src",
    "types",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "pnpm docs:build",
    "dev": "pnpm docs:dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint .",
    "type": "tsc --noEmit"
  },
  "dependencies": {
    "@unocss/preset-icons": "^0.46.5",
    "@vueuse/core": "^9.5.0",
    "less": "^4.1.3",
    "unocss": "^0.46.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.30.1",
    "@iconify-json/ri": "^1.1.3",
    "@types/node": "^18.11.9",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitepress": "1.0.0-alpha.29",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}